{
  "name": "express-reactjs-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for express as server and create react app as client",
  "main": "index.js",
  "scripts": {
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "client-build": "npm run build --prefix client",
    "client-init": "npm init --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "build": "npm run client-build",
    "init-project": "concurrently \"npm init \" \"npm client-init\""
  },
  "keywords": [
    "boilerplate",
    "react",
    "nodejs",
    "node",
    "create",
    "react",
    "app",
    "express",
    "expressjs"
  ],
  "author": "Muhamad Erza Wansyah",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "path": "^0.12.7",
    "uuid": "^8.3.2"
  }
}
